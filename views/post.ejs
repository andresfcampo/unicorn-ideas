<h2>What's the next unicorn?</h2>
<form action="/post/create" method="post">
  <input type="text" name="Startup Idea" required />
  <input type="text" name="description" required />
  <div>
    <input type="radio" name="private" value="false" checked />
    <label>public</label>
  </div>

  <div>
    <input type="radio" name="private" value="true" />
    <label>private</label>
  </div>

  <div>
    <label for="Industry">Choose an Industry:</label>
    <select id="industry" name="industry">
      <option value="fintech">Fintech</option>
      <option value="healthtech">Healthtech</option>
      <option value="edutech">Edutech</option>
      <option value="audi">Audi</option>
    </select>
  </div>
  <button type="submit">submit</button>
</form>